10%以下\", \"-10%以内\", \"0-10%\",\"10-30%\",\"30%以上\"],\n      // 展开字段集\n      key: \"年龄段\",\n      // key字段\n      value: \"人口数量\",\n      // value字段\n      retains: [\"State\",'Total'] ,// 保留字段集，默认为除fields以外的所有字段\n      callback(item, idx) {\n        const radio = idx / data.length;\n        return radio >= ds.state.start && radio <= ds.state.end;\n      },\n    });\n    return (\n      <div>\n        <Chart height={400} data={dv} scale={cols} forceFit>\n          <Legend />\n          <Coord  />\n          <Axis\n            name=\"State\"\n            label={{\n              offset: 12\n            }}\n            label={{\n\t\t\t\t\t\t\tformatter(text){\n\t\t\t\t\t\t\t\treturn text.length>8?text.substring(0,8)+'...':text\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t}}\n\n          />\n          <Axis name=\"人口数量\"\n         label={{\n          formatter(text){\n            return text.length>8?text.substring(0,8)+'...':text\n          }\n        }}\n\n          />\n          <Tooltip />\n          <Geom\n            type=\"intervalStack\"\n            position=\"State*人口数量\"\n            color={\"年龄段\"}\n            size={20}\n          >\n            <Label content={['Total*年龄段',(t,n)=>{\n                if(n==='小于5岁'){\n                    return t;\n                }\n            }]}/>\n          </Geom>\n        </Chart>\n        <Slider\n          data={dv}\n          padding={60}\n          xAxis=\"State\"\n          yAxis=\"Total\"\n          onChange={this.handleSliderChange}\n        />\n      </div>\n    );\n  }\n}\n\nexport default Stacked;\n","// data-set 可以按需引入，除此之外不要引入别的包\nimport React from 'react';\nimport { Chart, Axis, Tooltip, Geom, Legend, View } from 'bizcharts';\nimport DataSet from '@antv/data-set';\n\nimport * as echarts from 'echarts/core';\nimport {\n    ToolboxComponent,\n    TooltipComponent,\n    GridComponent,\n    LegendComponent\n} from 'echarts/components';\nimport {\n    BarChart\n} from 'echarts/charts';\nimport {\n    CanvasRenderer\n} from 'e