le: att,\n        loading: false\n      });\n    }\n  };\n  //资产投资经理\n  onChange3 = async (e) => {\n    let arr = this.state.investPeople.filter((item) => {\n      return item.saleUserId == e;\n    });\n    let fundCodes = arr[0].fundCodes;\n    this.setState({ loading: true });\n    let post = {};\n    post.fundCodes = fundCodes;\n    // post.fundCodes = ['SX9229','SX8530','SY7020']\n    const res = await getAllAssets(post);\n    if (+res.code == 1008) {\n      if (res.data.length == 0) {\n        this.setState({\n          showNoData: true,\n          loading: false\n        });\n        return false;\n      }\n      let att = [];\n      for (let i = 0; i < res.data.length; i++) {\n        let item = {};\n        item.tag = res.data[i].assetsName;\n        item.count = res.data[i].quotationLocal;\n        att.push(item);\n      }\n      this.setState({\n\n        showNoData: false,\n        customerNumberCircle: att,\n        loading: false\n      });\n    }\n  };\n  //行业投资经理\n  onChangeBR = async (e) => {\n    let arr = this.state.investPeople.filter((item) => {\n      return item.saleUserId == e;\n    });\n    let fundCodes = arr[0].fundCodes;\n    this.setState({ loading: true });\n    let post = {};\n    post.fundCodes = fundCodes;\n    // post.fundCodes = ['SX9229','SX8530','SY7020']\n    const res = await getIndustryQuotation(post);\n    if (+res.code == 1008) {\n      if (res.data.length == 0) {\n        this.setState({\n          showNoData2: true,\n          loading: false\n        });\n        return false;\n      }\n      let att = [];\n      for (let i = 0; i < res.data.length; i++) {\n        let item = {};\n        item.tag = res.data[i].industryName;\n        item.count = res.data[i].quotationLocal;\n        att.push(item);\n      }\n      this.setState({\n        showNo2Data: false,\n        customerNumberCircle2: att,\n        loading: false\n      });\n    }\n  }\n  //行业策略类型\n  onChangeBL = async (e) => {\n    let arr = this.state.type.filter((item) => {\n      return item.codelistId == e;\n    });\n    let fundCodes = arr[0].fundCodes;\n    this.setState({ loading: true });\n    let post = {};\n    //这里先写死\n    post.fundCodes = fundCodes;\n    //post.fundCodes = ['SX9229','SX8530','SY7020']\n    const res = await getIndustryQuotation(post);\n    if (+res.code == 1008) {\n      if (res.data.length == 0) {\n\n        this.setState({\n          showNoData2: true,\n          loading: false\n        });\n        return false;\n      }\n      let att = [];\n      for (let i = 0; i < res.data.length; i++) {\n        let item = {};\n        item.tag = res.data[i].industryName;\n        item.count = res.data[i].quotationLocal;\n        att.push(item);\n      }\n      this.setState({\n        showNoData2: false,\n        customerNumberCircle2: att,\n        loading: false\n      });\n    }\n  }\n  render() {\n\n    return (\n      <Card\n        bordered={false}\n        title={\n          // <span className={styles.cardTitle}>产品持仓结构</span>\n          <span className={styles.cardTitle}>产品持仓结构（敬请期待）</span>\n        }\n      >\n        <Spin spinning={this.state.loading} tip=\"页面正在加载中烦请等候，或刷新页面重新进入...\">\n          <div className={styles.card}>\n            <div className={styles.top}>\n              <div className={styles.left}>\n                <span className={styles.text}>资产类别市值统计</span>\n                <div style={{ marginTop: '20px;' }}>\n\n                  {\n                    this.state.showAssetsLeft ?\n                      <ChartOne picData={this.state.customerNumber} />\n                      :\n                      <div style={{ marginTop: '20px', height: '300px', textAlign: 'center', lineHeight: '300px' }}>\n                        <img src={NoData} alt=\"\" />\n                      </div>\n                  }\n                </div>\n              </div>\n              <div className={styles.right}>\n                <Radio.Group onChange={this.onChange} value={this.state.value} style={{ marginLeft: '10px' }}>\n                  <Row>\n                    <Col flex={2}>\n                      <Radio value={1} style={{ marginBottom: '10px' }}>\n                        策略类型\n                      </Radio>\n                      <Select\n                        showSearch\n                        style={{ marginLeft: '5px', marginRight: '5px' }}\n                        value={this.state.defaultTL}\n                        placeholder=\"请选择\"\n                        optionFilterProp=\"children\"\n                        onChange={this.onChange2}\n                        filterOption={(input, option) =>\n                          option.props.children.toLowerCase().indexOf(input.toLowerCase()) >= 0\n                        }\n                      >\n                        {\n                          this.state.type.map((item) => {\n                            return <Option key={item.codelistId} value={item.codelistId}>{item.typeName}</Option>;\n                          })\n                        }\n                      </Select>\n                    </Col>\n                    <Col flex={2}>\n                      <Radio value={2}>\n                        投资经理\n                      </Radio>\n                      <Select\n                        showSearch\n                        style={{ marginLeft: '5px' }}\n                        placeholder=\"请选择\"\n                        optionFilterProp=\"children\"\n                        onChange={this.onChange3}\n                        filterOption={(input, option) =>\n                          option.props.children.toLowerCase().indexOf(input.toLowerCase()) >= 0\n                        }\n                      >\n                        {\n                          this.state.investPeople.map((item) => {\n                            return <Option key={item.saleUserId} value={item.saleUserId}>{item.saleUserName}</Option>;\n                          })\n                        }\n                      </S