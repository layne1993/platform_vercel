io: '' };\n            item.type = '近三月';\n            item.changepercent = res.data.threeMonthNet.changepercent;\n            item.sharpeRatio = res.data.threeMonthNet.sharpeRatio;\n            arr.push(item);\n            let item2 = {};\n            item2.type = '近六月';\n            item2.changepercent = res.data.oneMonthNet.changepercent;\n            item2.sharpeRatio = res.data.oneMonthNet.sharpeRatio;\n            arr.push(item2);\n            let item3 = {};\n            item3.type = '近一年';\n            item3.changepercent = res.data.oneYearNet.changepercent;\n            item3.sharpeRatio = res.data.oneYearNet.sharpeRatio;\n            arr.push(item3);\n            let item4 = {};\n            item4.type = '今年以来';\n            item4.changepercent = res.data.fromThisYearNet.changepercent;\n            item4.sharpeRatio = res.data.fromThisYearNet.sharpeRatio;\n            arr.push(item4);\n            let item5 = {};\n            item5.type = '成立以来';\n            item5.changepercent = res.data.sinceFoundationNet.changepercent;\n            item5.sharpeRatio = res.data.sinceFoundationNet.sharpeRatio;\n            arr.push(item5);\n\n            let arrLeft = [];\n            arrLeft.push(res.data.threeMonthNet.changepercentRanking);\n            arrLeft.push(res.data.oneMonthNet.changepercentRanking);\n            arrLeft.push(res.data.oneYearNet.changepercentRanking);\n            arrLeft.push(res.data.fromThisYearNet.changepercentRanking);\n            arrLeft.push(res.data.sinceFoundationNet.changepercentRanking);\n            let maxL = arrLeft[0];\n            for (let i = 0; i < arrLeft.length; i++) {\n                maxL = maxL < arrLeft[i] ? arrLeft[i] : maxL;\n            }\n\n            let arrRight = [];\n            arrRight.push(res.data.threeMonthNet.sharpeRatioRanking);\n            arrRight.push(res.data.oneMonthNet.sharpeRatioRanking);\n            arrRight.push(res.data.oneYearNet.sharpeRatioRanking);\n            arrRight.push(res.data.fromThisYearNet.sharpeRatioRanking);\n            arrRight.push(res.data.sinceFoundationNet.sharpeRatioRanking);\n            let maxR = arrRight[0];\n            for (let i = 0; i < arrRight.length; i++) {\n                maxR = maxR < arrRight[i] ? arrRight[i] : maxR;\n            }\n\n            // 如果Left=0 表示所有字段的值都为null，此时页面显示无数据\n            let Left = 0;\n            if (res.data.sixMonthNet.changepercentRanking !== null || res.data.sixMonthNet.sharpeRatioRanking !== null) { Left = 1; }\n            if (res.data.threeMonthNet.changepercentRanking !== null || res.data.sixMonthNet.sharpeRatioRanking !== null) { Left = 1; }\n            if (res.data.oneYearNet.changepercentRanking !== null || res.data.sixMonthNet.sharpeRatioRanking !== null) { Left = 1; }\n            if (res.data.fromThisYearNet.changepercentRanking !== null || res.data.sixMonthNet.sharpeRatioRanking !== null) { Left = 1; }\n            if (res.data.sinceFoundationNet.changepercentRanking !== null || res.data.sixMonthNet.sharpeRatioRanking !== null) { Left = 1; }\n\n            if (Left == 0) {\n                this.setState({\n                    showLeft: false\n                });\n            } else {\n                this.setState({\n                    showLeft: true\n                });\n            }\n\n            for (let q = 0; q < arrLeft.length; q++) {\n                if (arrLeft[q] == null) {\n                    arrLeft[q] = 0;\n                }\n            }\n            for (let w = 0; w < arrRight.length; w++) {\n                if (arrRight[w] == null) {\n                    arrRight[w] = 0;\n                }\n            }\n            // console.log(arrLeft);\n            // console.log(arrRight);\n            // console.log(maxL);\n            // console.log(maxR);\n            this.setState({\n                loading: false,\n                tableData: arr,\n                dataCount: arrLeft,\n                dataRange: arrRight,\n                maxLeft: maxL + 1,\n  