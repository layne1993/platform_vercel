 @description: 表格变化\n    */\n    const _tableChange = (p, e, s) => {\n        const pageData = {\n            pageNum: p.current,\n            pageSize: p.pageSize\n        };\n        setPageData(pageData);\n        setSelectedRowKeys([]);\n        getListData({ ...form.getFieldsValue(), ...pageData });\n    };\n\n\n    // 表头\n    const columns = [\n        {\n            title: '适当性回访标题',\n            dataIndex: 'title',\n            width: 300,\n            fixed: 'left',\n            ellipsis: true\n            // render: (val) => val ? <p title={val}>{val}</p> : '--'\n        },\n        {\n            title: '回访比例',\n            dataIndex: 'percent',\n            width: 100,\n            render: (val) => val ? `${val}%` : '--'\n        },\n        {\n            title: '回访开始时间',\n            dataIndex: 'startTime',\n            width: 150,\n            render: (val) => val ? moment(val).format(date_format) : '--'\n        },\n        {\n            title: '对应产品',\n            dataIndex: 'productFullNameList',\n            width: 200,\n            // ellipsis: true\n            render: (val) => val ? val.join(',') : '--'\n        },\n        {\n            title: '回访完成度',\n            dataIndex: 'completionProgress',\n            width: 120,\n            render: (val) => val ? `${val}%` : '--'\n        },\n        {\n            title: '状态',\n            dataIndex: 'status',\n            width: 80,\n            render: (val) => STATUS[val] || '--'\n        },\n        {\n            title: '操作',\n            width: 100,\n            render: (record) => (\n                <Space>\n                    <Popconfirm title=\"是否禁用！\" onConfirm={() => updateStatus(record.investorSuitableReturnVisitId)}>\n                        {record.status === 1 && <a>禁用</a>}\n                    </Popconfirm>\n                </Space>\n            )\n        }\n    ];\n\n    const rowSelection = {\n        selectedRowKeys,\n        onChange: setSelectedRowKeys\n    };\n\n    return