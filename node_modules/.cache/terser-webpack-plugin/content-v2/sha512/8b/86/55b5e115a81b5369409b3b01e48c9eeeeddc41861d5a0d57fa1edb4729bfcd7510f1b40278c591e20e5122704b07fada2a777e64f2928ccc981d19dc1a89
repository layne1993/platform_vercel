\n    };\n\n    useEffect(getProductList, []);\n\n\n    /**\n     * @description 发起跳转 创建流程\n     * @param values\n     */\n    const onFinish = (values) => {\n        dispatch({\n            type: 'APPROPRIATE_RETURNVISIT/createReturnVisit',\n            payload: {\n                ...values,\n                startTime: moment().valueOf()\n            },\n            callback: (res) => {\n                const { code, data, message } = res;\n                if (code === 1008) {\n                    onSuccess();\n                } else {\n                    const warningText = message || res.data || '创建失败！';\n                    openNotification('error', `提示（代码：${res.code}）`, warningText, 'topRight');\n                }\n            }\n        });\n    };\n\n    // 选择风险等级\n    const onChange = (e) => {\n        let newProductList = [];\n        allProductList.map((item) => {\n            if (item.riskType === e) {\n                newProductList.push(item);\n            }\n        });\n        if (e === undefined) {\n            newProductList = allProductList;\n        }\n        form.setFieldsValue({ productIdList: undefined });\n        setProductList(newProductList);\n    };\n\n    return (\n        <Modal\n            title=\"产品适当性回访基本内容\"\n            visible={flag}\n            footer={null}\n            onCancel={onClose}\n            width={800}\n        >\n            <Form\n                // layout=\"vertical\"\n                form={form}\n                onFinish={onFinish}\n                {...formItemLayout}\n            >\n                <Form.Item\n                    label=\"此次适当性回访标题\"\n                    name=\"title\"\n                    rules={[{ required: true, message: '请输入回访标题' }]}\n                >\n                    <Input placeholder=\"请输入回访标题\" />\n                </Form.Item>\n                <Form.Item\n                    label=\"选择产品风险等级（选填）\"\n                    name=\"riskType\"\n                // rules={[{ required: true, message: '请选择' }]}\n                >\n                    <Select\n                        allowClear\n                        placeholder=\"请选择\"\n                        onChange={onChange}\n                    >\n                        {XWFundRiskLevel.map((item) => (\n                            <Select.Option value={item.value} key={item.value}>\n                                {item.label}\n                            </Select.Option>\n                        ))}\n                    </Select>\n                </Form.Item>\n                <Form.Item\n                    label=\"选择回访产品\"\n                    name=\"productIdList\"\n                    rules={[{ required: true, message: '请选择' }]}\n                >\n                    <Select\n                        mode=\"multiple\"\n                        allowClear\n                        showSearch\n                        placeholder=\"请选择产品\"\n                        style={{ maxWidth: 400 }}\n                        filterOption={(input, option) =>\n                            option.children.toLowerCase().indexOf(input.toLowerCase()) >= 0\n                        }\n                    >\n                        {productList.map((item) => (\n                            <Select.Option value={item.productId} key={item.productId}>\n                                {item.productName}\n                            </Select.Option>\n                        ))}\n                    </Select>\n                </Form.Item>\n                <Form.Item\n                    label=\"回访开始时间\"\n                // name=\"startTime\"\n                >\n                    <Input disabled placeholder=\"默认保存的那一刻，回访时间开始\" />\n                </Form.Item>\n                <Form.Item\n                    label=\"选择回访人数比例（%）\"\n                    name=\"percent\"\n                    rules={[{ required: true, message: '请输入' }]}\n                >\n                    <InputNumber min={0} />\n                </Form.Item