 'isInvestmentProduct',\n            width: 120,\n            render: (val, record) => {\n                if (isNumber(val)) {\n                    let text = val === 1 ? '是' : '否';\n                    return <span>{text}</span>;\n                } else {\n                    return '--';\n                }\n            }\n        },\n        {\n            title: '产品成立日',\n            dataIndex: 'setDate',\n            width: 120,\n            // sorter: true,\n            render: (val) => <span>{(val && moment(val).format(dateFormat)) || '--'}</span>\n        },\n        {\n            title: '最新净值日期',\n            dataIndex: 'netDate',\n            width: 150,\n            // sorter: true,\n            render: (val) => <span>{(val && moment(val).format(dateFormat)) || '--'}</span>\n        },\n        {\n            title: '最新单位净值',\n            dataIndex: 'netValue',\n            width: 150,\n            // sorter: true,\n            render: (val) => <span>{val || '--'}</span>\n        },\n        {\n            title: '最新累计净值',\n            dataIndex: 'acumulateNetValue',\n            width: 150,\n            // sorter: true,\n            render: (val) => <span>{val || '--'}</span>\n        },\n        {\n            title: '销售方式',\n            dataIndex: 'saleByProxy',\n            width: 150,\n            render: (val) => {\n                if (val === '0') {\n                    return <span>直销</span>;\n                } else if (val === '1') {\n                    return <span>代销</span>;\n                } else if (val === '0,1') {\n                    return <span>直销、代销</span>;\n                }\n                return <span>--</span>;\n            }\n        },\n        {\n            title: '操作',\n            dataIndex: 'age',\n            // fixed: 'right',\n            width: 150,\n            render: (text, record) => {\n                // eslint-disable-next-line no-undef\n                const { authEdit } =\n                    (sessionStorage.ge