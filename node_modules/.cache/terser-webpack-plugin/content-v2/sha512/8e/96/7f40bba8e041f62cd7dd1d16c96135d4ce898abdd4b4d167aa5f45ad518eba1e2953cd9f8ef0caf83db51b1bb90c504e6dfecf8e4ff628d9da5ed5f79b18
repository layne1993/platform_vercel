n    useEffect(() => {\n        let newData = [];\n        for (let i = 0; i < tableData.length; i++) {\n            let obj = { ...tableData[i] };\n            if (total) {\n                obj.rate = parseFloat(((100 * obj.endNetValue) / total).toFixed(2));\n            } else {\n                obj.rate = 0;\n            }\n            newData.push(obj);\n        }\n        setTableData(newData);\n    }, [total]);\n\n    useEffect(() => {\n        if (visible) {\n            let totalCount = 0;\n            let source = [];\n            if (modalType == 'xinpi') {\n                source = tableInfo?.filter((item) => item.positionType !== '总计') ?? [];\n                totalCount = source?.reduce((curr, next) => curr + next.endNetValue * 1, 0) ?? 0;\n            } else {\n                totalCount =\n                    tableSource?.reduce((curr, next) => curr + next.endNetValue * 1, 0) ?? 0;\n                source = tableSource?.map((item, index) => ({ ...item, id: index + 1 })) ?? [];\n            }\n            setTableData(source);\n            setTotal(totalCount);\n        } else {\n            setTableData([]);\n        }\n    }, [visible, tableSource, tableInfo]);\n\n    const handleSubmit = () => {\n        const newData = tableData.concat({\n            id: 9999,\n            positionType: '总计',\n            endNetValue: total,\n            rate: 100,\n        });\n        newData.forEach((item) => {\n            item.endNetValue = format((item.endNetValue * 1).toFixed(2));\n            item.rate = format((item.rate * 1).toFixed(2));\n        });\n        onSubmit(newData);\n    };\n\n    const tableColumns = [\n        {\n            title: '持仓类别',\n            dataIndex: 'positionType',\n        },\n        {\n            title: '期末市值（元）',\n            dataIndex: 'endNetValue',\n            align: 'center',\n            render: (text: any, record: any) =>\n                modalType == 'xinpi' ? (\n                    <span>{text}</span>\n                ) : (\n                    <Input type=\"number\" value={text} onChange={(e) => handleInput(e, record)} />\n                ),\n        },\n        {\n            title: '占资产比例（%）',\n            dataIndex: 'rate',\n            align: 'center',\n            render: (text) => <span>{text}%</span>,\n        },\n    ];\n    return (\n        <Modal\n            visible={visible}\n            title=\"核对资产分布权重\"\n            width={720}\n            onOk={handleSubmit}\n            onCancel={onCancel}\n        >\n            <div className={styles.container}>\n                <Table\n                    loading={false}\n                    rowKey=\"id\"\n                    size=\"middle\"\n                    columns={tableColumns}\n                    dataSource={tableData}\n                    pagination={false}\n                    summary={(pageData) => {\n                        let totalBorrow = 0;\n                        let totalRepayment = 0;\n                        pageData.forEach(({ endNetValue, rate }) => {\n                            totalBorrow += endNetValue * 1;\n                            totalRepayment += rate * 1;\n                        });\n                        totalBorrow = parseFloat(totalBorrow.toFixed(2));\n                        if (totalRepayment >= 99.99) totalRepayment = 100;\n                        return (\n                            <>\n                                <Table.Summary.Row>\n                                    <Table.Summary.Cell>总计</Table.Summary.Cell>\n                                    <Table.Summary.Cell align=\"center\">\n                                        <Text>{totalBorrow}</Text>\n                                    </Table.Summary.Cell>\n                                    <Table.Summary.Cell align=\"center\">\n                                        <Text>{totalRepayment}%</Text>\n                                    </Table.Summary.Cell>\n                                </Table.Summary.Row>\n                            </>\n                        );\n                    }}\n                />\n            </div>\n        </Modal>\n    );\n};\n\nexport default PieModal;\n","import React, { useEffect, useState, useRef } from 'react';\nimport { PageHeaderWrapper } from '@ant-design/pro-layout';\nimport axios from 'axios';\nimport { getCookie } from '@/utils/utils';\nimport { Card, Form, Button, DatePicker, Radio, Input, Select, Spin, message } from 'antd';\nimport { RestOutlined } from '@ant-design/icons';\nimport moment from 'moment';\nimport {\n    getMonthlyReportImgData,\n    monthlyReportgetTemplateList,\n    getReportTemplateAjax,\n    getReportColorData,\n    reportReveal,\n} from '../../../service';\nimport { setEchartsData, setPositionStep, setTopTen } from './monthChart/ruiChart'; // 中欧瑞博日报\nimport { setProductTrend, setPositionStepPie, setTopTenPie } from './monthChart/qingChart'; // 青骊月报\nimport { setNingquanTrend, setPositionPie, setIndustryBar } from './monthChart/ningChart'; // 宁泉月报\nimport { setNingquanTrendXT } from './monthChart/ningXTChart';\nimport { setMingShiTrend } from './monthChart/mingChart';\nimport MatchModal from '../components/MatchModal';\nimport PieModal from '../components/PieModal';\n\nimport styles from './index.less';\nimport { cloneDeep } from 'lodash';\n\nconst { RangePicker, WeekPicker } = DatePicker;\nconst { Option } = Select;\n\nconst GenerateReport: React.FC<{}> = (props) => {\n    const { parentForm, productNames } = props;\n    const [downloadLoading, setDownloadLoading] = useState(false);\n    const [tempalteArr, settempalteArr] = useState([]);\n    const [colorArr, setColorArr] = useState([]);\n    const [titleArr, setTitleArr] = useState([{ item: '', id: 1 }]);\n    const [count, setCount] = useState(2);\n    const [modalVisible, setModalVisible] = useState(false);\n    const [pieVisible, setPieVisible] = useState(false);\n    const [mingShiFlag, setMingShiFlag] = useState(false);\n    const [ningPosition, set