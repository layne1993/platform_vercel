gInfo, item.productId).then((str) => {\n                    performance = str;\n\n                    productIdImgList.push({\n                        productId: item.productId,\n                        imgInfo: {\n                            performance,\n                            asset,\n                            industry,\n                            productTrend: '',\n                        },\n                    });\n                    index++;\n                    if (index === res.data.length) resolve(productIdImgList);\n                });\n            });\n        }).then((productIdImgListStr) => {\n            _postDownload(`/mrp_analysis/monthlyReport/${urlDownLoad}`, {\n                ...params,\n                productIdImgListStr,\n            });\n        });\n    };\n\n    // 青骊月报画图\n    const getQingChart = (res, params) => {\n        const productIdImgList = [];\n        new Promise((resolve, reject) => {\n            let index = 0;\n            res.data.forEach(async (item) => {\n                let asset = '';\n                setPositionStepPie(item.imgInfo, item.productId).then((str) => {\n                    asset = str;\n                });\n                let industry = '';\n                setTopTenPie(item.imgInfo, item.productId).then((str) => {\n                    industry = str;\n                });\n\n                let performance = '';\n                setProductTrend(item.imgInfo, item.productId).then((str) => {\n                    performance = str;\n\n                    productIdImgList.push({\n                        productId: item.productId,\n                        imgInfo: {\n                            performance: '',\n                            productTrend: performance,\n                            asset,\n                            industry,\n                        },\n                    });\n                    index++;\n                    if (index === res.data.length) resolve(productIdImgList);\n                });\n            });\n        }).then((productIdImgListStr) => {\n            _postDownload(`/mrp_analysis/monthlyReport/${urlDownLoad.current}`, {\n                ...params,\n                productIdImgListStr,\n            });\n        });\n    };\n\n    //宁泉月报画图\n    const getNingChart = (res, params) => {\n        setPieVisible(true);\n        setNingPosition(res.data[0].imgInfo.ningquanPosition);\n        setNingParams({\n            resData: res.data,\n            params: params,\n        });\n    };\n\n    const getNingChartXT = (res, params) => {\n        setNingXTParams({ res: cloneDeep(res), params: cloneDeep(params) });\n        const productIdImgList = [];\n        new Promise((resolve, reject) => {\n            let num = 0;\n            res.data?.forEach(async (item, index) => {\n                let ningquanTrend = '';\n                setNingquanTrendXT(item.imgInfo, productNames[index]).then((str) => {\n                    ningquanTrend = str;\n                    productIdImgList.push({\n                        productId: item.productId,\n                        imgInfo: {\n                            ningquanTrend,\n                        },\n                    });\n                    num++;\n                    if (num === res.data.length) resolve(productIdImgList);\n                });\n            });\n        }).then((productIdImgListStr) => {\n            setisShowXTFlag(true);\n            // setImgSrc('data:image/png;base64,' + productIdImgListStr[0].imgInfo.ningquanIndustry)\n            _postDownload(`/mrp_analysis/monthlyReport/${urlDownLoad.current}`, {\n                ...params,\n                productIdImgListStr,\n            });\n        });\n    };\n\n    // 信托同步信披\n    const onDownReportRevealByXT = () => {\n        setModalType('xinpi');\n        setBtnLoading(true);\n        console.log('同步信披执行');\n        const productIdImgList = [];\n        new Promise((resolve, reject) => {\n            let num = 0;\n            ningXTParams?.res?.data?.forEach(async (item, index) => {\n                let ningquanTrend = '';\n                setNingquanTrendXT(item.imgInfo, productNames[index]).then((str) => {\n                    ningquanTrend = str;\n                    productIdImgList.push({\n                        productId: item.productId,\n                        imgInfo: {\n                            ningquanTrend,\n                        },\n                    });\n                    num++;\n                    if (num === ningXTParams?.res?.data?.length) resolve(productIdImgList);\n                });\n            });\n        }).then((productIdImgListStr) => {\n            _postDownloadXinpi(`/mrp_analysis/monthlyReport/reportReveal`, {\n                ...ningXTParams?.params,\n                productIdImgListStr,\n            });\n        });\n    };\n\n    const handleSubmitNingPie = (tableData) => {\n        setPieVisible(false);\n        if (modalType == 'xinpi') {\n            setTableInfo([]);\n        } else {\n            setTableInfo([...tableData] || []);\n        }\n\n        const productIdImgList = [];\n        new Promise((resolve, reject) => {\n            let num = 0;\n            ningParams.resData?.forEach(async (item, index) => {\n                let ningquanPosition = '';\n                setPositionPie(tableData, item.productId).then((str) => {\n                    ningquanPosition = str;\n                });\n                let ningquanIndustry = '';\n                setIndustryBar(item.imgInfo, item.productId).then((str) => {\n                    ningquanIndustry = str;\n                });\n                let ningquanTrend = '';\n                setNingquanTrend(item.imgInfo, productNames[index], item.productId).then((str) => {\n                    ningquanTrend = str;\n                    productIdImgList.push({\n                        productId: item.productId,\n                        imgInfo: {\n                            ningquanTrend,\n                            ningquanIndustry,\n                            ningquanPosition,\n                        },\n                    });\n                    num++;\n                    if (num === ningParams.resData.length) resolve(productIdImgList);\n                });\n            });\n        }).then((productIdImgListStr) => {\n            // setImgSrc('data:image/png;base64,' + productIdImgListStr[0].imgInfo.ningquanIndustry)\n            _postDownload(`/mrp_analysis/monthlyReport/${urlDownLoad.current}`, {\n                ...ningParams.params,\n                ningquanPositionData: tableData,\n                productIdImgListStr,\n            });\n        });\n    };\n\n    const handleCancelNingPie = () => {\n        setPieVisible(false);\n        setDownloadLoading(false);\n        setBtnLoading(false);\n    };\n\n    // 鸣石月报画图\n    const getMingChart = (res, params) => {\n        const productIdImgList = [];\n        new Promise((resolve, reject) => {\n            let index = 0;\n            res.data.forEach(async (item, index) => {\n                let mingshiTrend = '';\n                setMingShiTrend(item.imgInfo, params, productNames[index]).then((str) => {\n                    mingshiTrend = str;\n\n                    productIdImgList.push({\n                        productId: item.productId,\n                        imgInfo: {\n                            mingshiTrend,\n                        },\n                    });\n                    index++;\n                    if (index === res.data.length) resolve(productIdImgList);\n                });\n            });\n        }).then((productIdImgListStr) => {\n            _postDownload(`/mrp_analysis/monthlyReport/${urlDownLoad.current}`, {\n                ...params,\n                productIdImgListStr,\n            });\n        });\n    };\n\n    const _postDownload = (url, data) => {\n        console.log(modalType, 'modalType值为');\n        axios({\n            url: `${BASE_PATH.adminUrl}${url}`,\n            data,\n            method: 'post',\n            responseType: 'blob',\n            headers: {\n                'Content-Type': 'application/json; application/octet-stream',\n                tokenId: getCookie('vipAdminToken') || '',\n            },\n        })\n            .then((response) => {\n                if (modalType == 'xinpi') {\n                    message.success('同步信披成功');\n                    setBtnLoading(false);\n                    setisShowXTFlag(false);\n                    setNingXTParams({});\n                    return;\n                }\n                let fileName = response.headers['content-disposition']\n                    .split(';')[1]\n                    .split('filename=')[1]\n                    .split('.doc')[0];\n                fileName = decodeURIComponent(fileName); // eslint-disable-next-line no-undef\n                let blob = new Blob([response.data], {\n                    type: `application/msword`, //word文档为msword,pdf文档为pdf，msexcel 为excel\n                });\n                let downloadElement = document.createElement('a');\n                let href = window.URL.createObjectURL(blob);\n                downloadElement.href = href;\n                downloadElement.download = fileName;\n                document.body.appendChild(downloadElement);\n                downloadElement.click();\n                document.body.removeChild(downloadElement);\n                window.URL.revokeObjectURL(href);\n                setDownloadLoading(false);\n            })\n            .catch(function (error) {\n                console.log(modalType, 'modalType值为');\n                message.warning('下载失败!');\n                setDownloadLoading(false);\n                setBtnLoading(false);\n                setisShowXTFlag(false);\n                setNingXTParams({});\n            });\n    };\n\n    const _postDownloadXinpi = (url, data) => {\n        axios({\n            url: `${BASE_PATH.adminUrl}${url}`,\n            data,\n            method: 'post',\n            responseType: 'blob',\n            headers: {\n                'Content-Type': 'application/json; application/octet-stream',\n                tokenId: getCookie('vipAdminToken') || '',\n            },\n        })\n            .then((response) => {\n                message.success('同步信披成功');\n                setBtnLoading(false);\n                setisShowXTFlag(false);\n                setNingXTParams({});\n            })\n            .catch((err) => {\n                message.warning('同步信披失败');\n                setBtnLoading(false);\n                setisShowXTFlag(false);\n                setNingXTParams({});\n            });\n    };\n\n    //\n    const handleChangeTemplate = (val) => {\n        settempFlagName(val);\n    };\n\n    const layout = {\n        labelCol: { span: 4 },\n        wrapperCol: { span: 10 },\n    };\n\n    const radioArr = [\n        { label: '根号250', value: '250' },\n        { label: '根号252', value: '252' },\n    ];\n\n    useEffect(() => {\n        form.setFieldsValue({\n            flag: '250',\n            standard: 4978,\n            entity: '沪深300',\n        });\n\n        // getMonthlyReportgetTemplateList()\n        // 请求模板\n        getReportTemplate({ reportForm: 2, reportDimension: 2 });\n    }, []);\n\n    // const [imgSrc, setImgSrc] = useState('');\n    // useEffect(() => {}, [isDownReport]);\n\n    return (\n        <>\n            <Form form={form} {...layout}>\n                <div className={styles.boxItem}>\n                    <div className={styles.topTitle}>产品指标设置</div>\n                    <Form.Item name=\"flag\" label=\"指标年化方式\">\n                        <Radio.Group options={radioArr} />\n                    </Form.Item>\n\n                    <Form.Item name=\"percent\" label=\"无风险收益率\">\n                        <Input suffix=\"%\" />\n                    </Form.Item>\n\n                    {mingShiFlag ? (\n                        <Form.Item name=\"standard\" 