map((item, index) => <Option key={index} value={item.value}>{item.label}</Option>)\n                                }\n                            </Select>\n                        </FormItem>\n                        <FormItem\n                            {...formItemLayout}\n                            label=\"文件权限\"\n                            name=\"fileAuthority\"\n                            rules={[\n                                {\n                                    required: true,\n                                    message: '请选择文件权限'\n                                }\n                            ]}\n                            extra=\"文件权限有：持有人可见，合规投资者可见，风险测评客户可见\"\n                        >\n                            <Select placeholder=\"请选择\" mode=\"multiple\" allowClear>\n                                {\n                                    XWFileAuthority.map((item) => {\n                                        return (\n                                            <Option key={getRandomKey(5)} value={item.value}>{item.label}</Option>\n                                        );\n                                    })\n                                }\n                            </Select>\n                        </FormItem>\n\n                        <FormItem\n                            {...formItemLayout}\n                            label=\"文件上传\"\n                            name=\"file\"\n                            rules={[\n                                {\n                                    required: !(fileList.length > 0),\n                                    message: '请上传文件'\n                                }\n                            ]}\n                            extra=\"请上传ZIP文件\"\n                        >\n                            <Upload\n                                name=\"file\"\n                                headers={{\n                                    tokenId: getCookie('vipAdminToken')\n                                }}\n                                accept=\".zip\"\n                                beforeUpload={this.beforeUpload}\n                                fileList={fileList}\n                                onRemove={this.onRemove}\n                                showUploadList={this.props.authExport}\n                                // beforeUpload={this.beforeUpload}\n                                // onPreview={this.filePreView}\n                                // onChange={this.handleFileChange}\n                                disabled={!this.props.authEdit}\n                            >\n                                <Button disabled={!this.props.authEdit}>\n                                    <UploadOutlined /> 上传文件\n                                </Button>\n                            </Upload>\n                        </FormItem>\n                        <FormItem\n                            {...submitFormLayout}\n                            style={{\n                                marginTop: 32\n                            }}\n                        >\n                            {\n                                this.props.authEdit &&\n                                <Button\n                                    // disabled={fileInfomation.startStatus === 1}\n                                    type=\"primary\" htmlType=\"submit\"\n                                    loading={loading}\n                                    onClick={() => { this.setState({ save: 1 }); }}\n                                    style={{ marginLeft: 8, marginRight: 8 }}\n                                >\n                                    确定\n                                </Button>\n                            }\n                            <Button onClick={onCancel}>\n                                取消\n                            </Button>\n                        </FormItem>\n                    </Form>\n                </Spin>\n\n            </Modal>\n        );\n    }\n}\n\nexport default connect(({ productNoticeBatch, loading }) => ({\n    productNoticeBatch,\n    loading: loading.effects['productNoticeBatch/productFileUpload']\n}))(FileInfo);\n","/*\n * @description: 文件信息\n * @Author: tangsc\n * @Date: 2020-11-02 10:45:29\n */\nimport { Button, Form, Row, Input, Select, Col, Dropdown, notification, DatePicker, Modal, Menu, Space, Tooltip,Checkbox } from 'antd';\nimport { connect, history } from 'umi';\nimport React, { Fragment, Component } from 'react';\nimport { getCookie, exportFile, fileExport } from '@/utils/utils';\nimport { FILE_TYPE, XWFileAuthority } from '@/utils/publicData';\nimport moment from 'moment';\nimport styles from '../details/components/styles/Tab3.less';\nimport { ExclamationCircleOutlined, PlusOutlined, DownOutlined, InfoCircleOutlined } from '@ant-design/icons';\nimport { isEmpty } from 'lodash';\nimport { MultipleSelect } from '@/pages/components/Customize';\nimport MXTable from '@/pages/components/MXTable';\nimport { GridContent, PageHeaderWrapper } from '@ant-design/pro-layout';\nimport MXAnnouncement from '@/pages/components/MXAnnouncement';\nimport FileInfoDetails from '../fileInfo';\nimport BatchAdd from './batchAdd';\n\nconst openNotification = (type, message, description, placement, duration) => {\n    notification[type || 'info']({\n        message, description, placement, duration: duration || 3\n    });\n};\nconst { Option } = Select;\nconst FormItem = Form.Item;\nconst { confirm } = Modal;\nconst { authEdit, authExport } = sessionStorage.getItem('PERMISSION') && JSON.parse(sessionStorage.getItem('PERMISSION'))['30103'] || {};\n// 设置日期格式\nconst dateFormat = 'YYYY/MM/DD HH:mm:ss';\n\nclass Tab3 extends Component {\n    state = {\n        datasources: {},\n        powerInfo: {},\n        pageData: {\n            pageNum: 1,\n            pageSize: 20\n        },\n        selectedRowKeys: [],                 // 选中table行的key值\n        isModalVisible: false,               // 控制新增净值弹窗显示隐藏\n        isAddOrEdit: 'add',                  // 判断新增或者编辑\n        productFileId: 0,                     // 文件id\n        announcementModalFlag: false,         // 模板模态框flag\n        batchVisible: false, // 批量上传\n        productId: '',\n        allStatus:0,//设置单页面所有状态\n    };\n\n    componentDidMount() {\n        this.Interfacesearch();\n    }\n\n    searchFormRef = React.createRef();\n\n    columns = [\n        {\n            title: '产品名称',\n            dataIndex: 'productName',\n            render: (val, record) => <a onClick={() => history.push(`/product/list/details/${record.productId}`)}>{val || '--'}</a>,\n            width: 100\n        },\n        {\n            title: '文件名称',\n            dataIndex: 'documentName',\n            render: (val, record) => <a onClick={() => this._handleEdit(record)}>{val || '--'}</a>,\n            width: 100\n        },\n        {\n            title: <span><Tooltip placement=\"top\" title={'默认会读取母基金的所有文件'}><InfoCircleOutlined /></Tooltip>&nbsp;母基金文件</span>,\n            dataIndex: 'isParentFund',\n            render: (val) => <span>{val || '--'}</span>,\n            width: 110\n        },\n        {\n            title: '文件类型',\n            dataIndex: 'documentType',\n            render: (val) => <span>{val && FILE_TYPE.find((item) => (item.value === val)).label || '--'}</span>,\n            width: 120\n        },\n        {\n            title: '文件来源',\n            dataIndex: 'sourceFile',\n            width: 120,\n            render: (val) => {\n                if(val==1){\n                    return <span>后台创建</span>\n                }else if(val==2){\n                    return <span>易报表</span>\n                }\n                return <span>--</span>\n            }\n        },\n        {\n            title: '文件权限',\n            dataIndex: 'fileAuthority',\n            width: 150,\n            render: (val) => {\n                if (!isEmpty(val)) {\n                    let renderText = '';\n                    val.forEach((item) => {\n                        renderText = `${renderText}${XWFileAuthority[item - 1].label}，`;\n                    });\n                    return <span>{renderText.substring(0, renderText.length - 1) || '--'}</span>;\n                }\n            }\n            // width:120,\n        },\n        {\n            title: ()=>{\n                return (\n                    <>\n                    <Checkbox\n                        disabled={!authEdit}\n                        checked={this.state.allStatus}\n                        onChange={() => this._checkedChangeAll()}\n                        style={{marginRight:'8px'}}\n                    />\n                    \n                    <span>是否披露</span>\n                    </>\n                )\n            },\n            dataIndex: 'publishStatus',\n            width: 100,\n            render: (text, record) => {\n                return (\n                    <Checkbox\n                        disabled={!authEdit}\n                        checked={text === 1}\n                        onChange={() => this._checkedChange(text, record)}\n                    />\n                );\n            }\n        },\n        // {\n        //     title: '发布状态',\n        //     dataIndex: 'startStatus',\n        //     render: (val) => <span>{val === 0 && '未发布' || val === 1 && '已发布' || '--'}</span>\n        //     // width:100,\n        // },\n        {\n            title: '发布人',\n            dataIndex: 'userName',\n            render: (val) => <span>{val || '--'}</span>,\n            width: 100\n        },\n        {\n            title: '月报同步时间',\n            dataIndex: 'synchronizationTime',\n            render: (val) => <span>{val && moment(val).format(dateFormat) || '--'}</span>,\n            width: 120\n        },\n        {\n            title: '发布时间',\n            dataIndex: 'publishTime',\n            render: (val) => <span>{val && moment(val).format(dateFormat) || '--'}</span>,\n            width: 120\n        },\n        {\n            title: '操作',\n            width: 100,\n            render: (_, record) => {\n                return (\n                    <Fragment>\n                        {\n                            authEdit &&\n                            <div>\n                                <a onClick={() => this._handleEdit(record)}>查看</a>\n                                <a style={{ marginLeft: 8 }} onClick={() => this.deleteFile(record.productFileId)}>删除</a>\n                                {record.attachment && (record.attachment.signUrl ? <a href={`${record.attachment.signUrl}`} download={record.documentName} target=\"_blank\">&nbsp;&nbsp;下载</a> : <a href={`${record.attachment.baseUrl}`} download={record.documentName} target=\"_blank\">&nbsp;&nbsp;下载</a>)}\n                            </div>\n                        }\n\n                    </Fragment>\n                );\n            }\n        }\n    ];\n\n    _handleDownload = (url) => {\n        exportFile(url);\n    }\n\n    deleteFile = (id) => {\n        const { dispatch } = this.props;\n        const _this = this;\n        confirm({\n            title: '请您确认是否删除?',\n            icon: <ExclamationCircleOutlined />,\n            content: '删除后该文件信息会全部删除',\n            okText: '确认',\n            cancelText: '取消',\n            onOk() {\n                dispatch({\n                    type: 'productNotice/deleteProductFile',\n                    payload: {\n                        productFileId: id\n                    },\n                    callback: (res) => {\n                        if (res.code === 1008) {\n                            openNotification('success', '提示', '删除成功', 'topRight');\n                            _this.Interfacesearch();\n                        } else {\n                            const warningText = res.message || res.data || '查询失败！';\n                            openNotification('warning', `提示(代码：${res.code})`, warningText, 'topRight');\n                        }\n                    }\n                });\n            },\n            onCancel() {\n                // console.log('Cancel');\n            }\n        });\n    }\n    _checkedChange=(text, record)=>{\n        console.log(text, record); \n        const { dispatch } = this.props;\n        dispatch({\n             type: 'productNotice/updatePublistStatus',\n             payload: {\n                 productFileId:record.productFileId,\n                 publishStatus:text==0?1:0\n             },\n             callback: (res) => {\n \n                 if (+res.code === 1008) {\n                     this.Interfacesearch()\n                     openNotification('success', '提示', '披露状态修改成功', 'topRight');\n                 } else {\n                     const warningText = res.message || res.data || '披露状态修改失败！';\n                     openNotification('warning', `提示(代码：${res.code})`, warningText, 'topRight');\n                 }\n             }\n         });\n     }\n     //批量修改状态\n     _checkedChangeAll=()=>{\n         \n         let newArr = [] //保存当前页面所有id\n         for(let i=0;i<this.state.datasources.list.length;i++){\n             newArr.push(this.state.datasources.list[i].productFileId)\n         }\n         const { dispatch } = this.props;\n         this.setState({\n            allStatus:this.state.allStatus==1?0:1\n        },()=>{\n           dispatch({\n               type: 'productNotice/updatePublistStatus',\n               payload: {\n                   productFileId:newArr,\n                   publishStatus:this.state.allStatus\n               },\n               callback: (res) => {\n   \n                   if (+res.code === 1008) {\n                       this.Interfacesearch()\n                       openNotification('success', '提示', '披露状态批量修改成功', 'topRight');\n                   } else {\n                       const warningText = res.message || res.data || '披露状态批量修改失败！';\n                       openNotification('warning', `提示(代码：${res.code})`, warningText, 'topRight');\n                   }\n               }\n           });\n        })\n     }\n\n    /**\n     * @description 查询数据\n     */\n    Interfacesearch = () => {\n        // alert(2)\n        const { dispatch, params } = this.props;\n        const { pageData } = this.state;\n        const fromData = this.searchFormRef.current.getFieldsValue();\n        dispatch({\n            type: 'productNotice/getProductFile',\n            payload: {\n                ...fromData,\n                publishTime: fromData.publishTime && moment(fromData.publishTime).valueOf(),\n                ...pageData,\n                fileType:2,\n            },\n            callback: (res) => {\n\n                if (res.code === 1008 && res.data) {\n                    this.setState({\n                        datasources: res.data\n                    });\n                } else {\n                    const warningText = res.message || res.data || '查询失败！';\n                    openNotification('warning', `提示(代码：${res.code})`, warningText, 'topRight');\n                }\n            }\n        });\n    }\n\n    // 重置搜索\n    resetSearch = () => {\n        this.searchFormRef.current.resetFields();\n        // eslint-disable-next-line react/no-direct-mutation-state\n        this.state.pageData.pageNum = 1;\n        this.Interfacesearch();\n    }\n\n    // 搜索\n    handerSearch = () => {\n        this.Interfacesearch();\n    }\n\n    /**\n     * @description: 表格变化\n     */\n    _tableChange = (p, e, s) => {\n        console.log(p, e, s);\n        // eslint-disable-next-line react/no-direct-mutation-state\n        // this.state.pageData.pageNum = p.current;\n        // this.state.pageData.pageSize = p.pageSize;\n\n        this.setState({\n            pageData: {\n                pageNum: p.current,\n                pageSize: p.pageSize\n            }\n        }, () => {\n            this.Interfacesearch();\n        });\n\n\n    }\n\n\n    /**\n     * @description: Table的CheckBox change事件\n     * @param {Array} selectedRowKeys\n     */\n    _onSelectChange = (selectedRowKeys, selectRows) => {\n        console.log(selectedRowKeys, selectRows);\n        this.setState({\n            selectedRowKeys\n        });\n    };\n\n    /**\n     * @description: 清空已勾选\n     */\n    _cleanSelectedKeys = () => {\n        this.setState({\n            selectedRowKeys: []\n        });\n    };\n\n\n    /**\n     * @description: 批量导出产品信息\n     */\n    _export = () => {\n        const { selectedRowKeys } = this.state;\n        window.location.href = `${window.location.origin}${BASE_PATH.adminUrl}/productFile/download?productFileIds=${selectedRowKeys.toString()}&tokenId=${getCookie('vipAdminToken')}`;\n    }\n\n\n    // 导出全部\n    _downloadAll = () => {\n        const { pageData } = this.state;\n        const fromData = this.searchFormRef.current.getFieldsValue();\n\n        fileExport({\n            method: 'post',\n            url: '/productFile/allExport',\n            data: {\n                ...fromData,\n                publishTime: fromData.publishTime && moment(fromData.publishTime).valueOf(),\n                ...pageData\n            },\n            callback: ({ status }) => {\n                if (status === 'success') {\n                    openNotification('success', '提醒', '下载成功');\n                }\n                if (status === 'error') {\n                    openNotification('error', '提醒', '下载失败！');\n                }\n            }\n        });\n    }\n\n    /**\n     * @description: 文件编辑\n     * @param {*}\n     */\n    _handleEdit = (record) => {\n        this.setState({\n            isAddOrEdit: 'edit',\n            productFileId: record.productFileId,\n            isModalVisible: true,\n            productId: record.productId\n        });\n    }\n\n\n    /**\n     * @description: 新增文件\n     */\n    _onAdd = () => {\n        this.setState({\n            productId:'',\n            isModalVisible: true,\n            isAddOrEdit: 'add'\n        });\n    };\n\n    /**\n     * @description: 批量上传\n     */\n    batchAdd = () => {\n        this.setState({ batchVisible: true });\n    };\n\n    /**\n     * @description: 关闭新增、编辑文件弹窗\n     * @param {*}\n     */\n    _handleClose = () => {\n        this.setState({ isModalVisible: false });\n    }\n\n    /**\n     * @description: 关闭新增、编辑文件弹窗\n     * @param {*}\n     */\n    batchClose = () => {\n        this.setState({ batchVisible: false });\n    }\n\n    renderForm = () => {\n        const { powerInfo, selectedRowKeys } = this.state;\n        return (\n            <Form\n                hideRequiredMark\n                style={{\n                    marginTop: 8\n                }}\n                ref={this.searchFormRef}\n                onFinish={this.handerSearch}\n            >\n                <Row gutter={[20, 0]}>\n                    <Col span={8}>\n                        <FormItem\n                            label=\"文件名称\"\n                            name=\"documentName\"\n                        >\n                            <Input placeholder=\"请输入文件名称\" autoComplete=\"off\" />\n                        </FormItem>\n                    </Col>\n                    <Col span={8}>\n                        <FormItem\n                            label=\"文件来源\"\n                            name=\"sourceFile\"\n                        >\n                            <Select placeholder=\"请选择\" allowClear>\n                               <Option key={1} value={1}>后台创建</Option>\n                               <Option key={2} value={2}>易报表</Option>\n                            </Select>\n                        </FormItem>\n                    </Col>\n                    <Col span={8}>\n                        <FormItem\n                            label=\"披露状态\"\n                            name=\"publishStatus\"\n                        >\n                            <Select placeholder=\"请选择\" allowClear>\n                                <Option key={0} value={0}>不披露</Option>\n                                <Option key={1} value={1}>披露</Option>\n                            </Select>\n                        </FormItem>\n                    </Col>\n                   \n                    <Col span={8}>\n                        <FormItem\n                            label=\"发布时间\"\n                            name=\"publishTime\"\n                        >\n                            <DatePicker style={{ width: '100%' }} />\n                        </FormItem>\n                    </Col>\n                    <Col span={8}>\n                    </Col>\n                   \n                    <Col span={8} style={{ textAlign: 'right' }}>\n                        <FormItem>\n                            <Button type=\"primary\" htmlType=\"submit\" loading={false}>\n                                查询\n                            </Button>\n\n                            <Button style={{ marginLeft: 8 }} onClick={this.resetSearch}>\n                                重置\n                            </Button>\n                        </FormItem>\n                    </Col>\n                </Row>\n                <Row justify=\"space-between\">\n                    <Space>\n                        {\n                            authEdit &&\n                            <Button type=\"primary\" icon={<PlusOutlined />} onClick={this._onAdd}>\n                                新建\n                            </Button>\n                        }\n                        {\n                            authEdit &&\n                            <Button type=\"primary\" onClick={this.batchAdd}>\n                                批量上传\n                            </Button>\n                        }\n                        {\n                            authExport &&\n                            <Dropdown\n                                overlay={<Menu>\n                                    <Menu.Item\n                                        key=\"1\"\n                                        disabled={selectedRowKeys.length === 0}\n                                        onClick={this._export}\n                                    >\n                                        下载选中\n                                    </Menu.Item>\n                                    <Menu.Item\n                                        key=\"0\"\n                                        onClick={this._downloadAll}\n                                    >\n                                        下载全部\n                                    </Menu.Item>\n                                </Menu>}\n                            >\n                                <Button >\n                                    &nbsp;&nbsp;批量下载\n                                    <DownOutlined />\n                                </Button>\n                            </Dropdown>\n                            // <Button onClick={this._export} disabled={isEmpty(selectedRowKeys)}>\n                            //     批量下载\n                            // </Button>\n                        }\n                        {\n                            authEdit &&\n                        <a onClick={() => this.announcementModalSwitch(true)}>自动生成公告文件</a>\n                        }\n                    </Space>\n\n\n                </Row>\n            </Form>\n        );\n    }\n\n    // 模板模态框\n    announcementModalSwitch = (flag) => {\n        this.setState({\n            announcementModalFlag: flag\n        });\n    }\n\n    render() {\n        const { loading, params } = this.props;\n        const { datasources, productId, pageData, selectedRowKeys, isModalVisible, isAddOrEdit, productFileId, announcementModalFlag, batchVisible } = this.state;\n\n        const rowSelection = {\n            selectedRowKeys,\n            onChange: this._onSelectChange\n        };\n\n        return (\n            <PageHeaderWrapper title=\"产品公告文件\">\n                <div className={styles.container}>\n                    <div style={{ marginTop: 20, marginBottom: 20 }}>{this.renderForm()}</div>\n                    <MXTable\n                        loading={loading}\n                        columns={this.columns}\n                        dataSource={datasources && datasources.list}\n                        total={datasources.pageNum && datasources.total}\n                        pageNum={pageData.pageNum}\n                        rowKey={(record) => record.productFileId}\n                        onChange={(p, e, s) => this._tableChange(p, e, s)}\n                        rowSelection={rowSelection}\n                        sticky\n                        scroll={{ x: '100%', scrollToFirstRowOnChange: true }}\n                    />\n                    {\n                        isModalVisible &&\n                    <FileInfoDetails\n                        modalVisible={isModalVisible}\n                        onCancel={this._handleClose}\n                        productFileId={productFileId}\n                        operateType={isAddOrEdit}\n                        authEdit={authEdit}\n                        authExport={authExport}\n                        productId={productId}\n                        Interfacesearch={this.Interfacesearch}\n                        showProduct\n                    />\n                    }\n\n                    {\n                        announcementModalFlag && <MXAnnouncement disabled=\"1\" params={params} modalFlag={announcementModalFlag} closeModal={() => this.announcementModalSwitch(false)} updatelist={() => this.Interfacesearch()} />\n                    }\n                    {\n                        batchVisible && (\n                            <BatchAdd\n                                modalVisible={batchVisible}\n                                onCancel={this.batchClose}\n                                productFileId={productFileId}\n                                authEdit={authEdit}\n                                authExport={authExport}\n                                showProduct\n                                Interfacesearch={this.Interfacesearch}\n                            />)\n                    }\n                </div>\n            </PageHeaderWrapper>\n        );\n    }\n\n}\n\nexport default connect(({ loading }) => ({\n    loading: loading.effects['productNotice/getProductFile']\n}))(Tab3);\n"]}}