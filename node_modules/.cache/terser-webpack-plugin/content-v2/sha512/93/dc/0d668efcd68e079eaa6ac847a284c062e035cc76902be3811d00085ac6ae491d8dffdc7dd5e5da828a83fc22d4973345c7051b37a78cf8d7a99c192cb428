formData.append('pathUrl', pathArr.join('/'));\n        }\n        let res = await request.postMultipart(url, formData, { onUploadProgress: onUploadProgress },{\n            'companyCode':getCookie('companyCode'),\n            dataSource:'report'\n        });\n        console.log(res,'res')\n\n        const {code,data} = res.data\n        if (+code === 1001 && !data.checkFlag) {\n            setStatus(Status.Success);\n            callback(file);\n        } else {\n            setStatus(Status.Exception);\n            // 估值表已经解析的判断\n            if(+data.checkFlag ===2){\n                visibleObj[key] = true\n                dataSourceObj[key] = [data.parsedData]\n                setdataSourceObj({...dataSourceObj})\n                setvisibleObj({...visibleObj})\n            }\n\n            // 估值表未解析的判断\n            if(+data.checkFlag ===1){\n                message.warning(data.msg)\n            }\n\n            // 其他报错\n            console.log(data,'data 值为')\n            if(+data.checkFlag !==2 && +data.checkFlag !==1){\n                message.warning(data.msg)\n            }\n        }\n    };\n\n    const onCancel =()=>{\n        visibleObj[key] = false\n        setvisibleObj({...visibleObj})\n    }\n\n    // 覆盖上传\n    const onOk =async ()=>{\n        const formData = new window.FormData();\n        if (params) {\n            for (let key in params) {\n                formData.append(key, params[key]);\n            }\n        }\n        formData.append('valuationTableFiles', file);\n        if (file.webkitRelativePath) {\n            let pathArr = file.webkitRelativePath.split('/');\n            pathArr.pop();\n            formData.append('pathUrl', pathArr.join('/'));\n        }\n        const res = await request.postMultipart('/mvt/valuationTable/cover', formData, { onUploadProgress: onUploadProgress },{\n            'companyCode':getCookie('companyCode'),\n            dataSource:'report'\n        });\n        const {code} = res.data\n        if (+code === 1001) {\n            setStatus(Status.Success);\n            callback(file);\n        }\n        visibleObj[key] = false\n        setvisibleObj({...visibleObj})\n    }\n\n    useEffect(() => { doUpload(); }, []);\n\n    return (\n        <div>\n        <div className={_styles.progressWarp}>\n            <Progress percent={percent} status={status} />\n            <p>{file.webkitRelativePath ? file.webkitRelativePath : file.name}</p>\n        </div>\n\n        <Modal\n        title=\"提示\"\n        okText=\"覆盖\"\n        visible={visibleObj[key]}\n        onCancel={onCancel}\n        onOk={onOk}\n        >\n            文件已存在，请选择是否覆盖原文件？\n            <br/>原文件内容：\n            <Table columns={columns} dataSource={dataSourceObj[key]} />\n        </Modal>\n        </div>\n    );\n};\n\n\nexport default ProgressBar;\n","/*\n * @Descripttion: 进度条\n * @version:\n * @Author: yezi\n * @Date: 2021-04-22 16:26:25\n * @LastEditTime: 2021-08-16 13:36:59\n */\n\nimport React, { useState } from 'react';\nimport type { FC } from 'react';\nimport { Upload, Modal } from 'antd';\nimport Progress from './Progress';\nimport axios from 'axios';\nimport { getToken } from '@/utils/utils';\nimport lodash from 'lodash';\n\nconst maxNum = 3;\n\nlet queueFileList = [];\n\nlet uploadDone = false;\n\nconst MAX_FILE_SIZE = 1024 * 1024 * 100;\n\nconst UploadProgress: FC<any> = (props) => {\n    const { uploadProps, params, url, children, onSuccess } = props;\n    // const [queue, setQueue] = useState<any[]>([]);                                                    // 上传的队列\n    const [uploadingQueue, setUploadingQueue] = useState<any[]>([]);                                  // 上传的队列\n    const [progress, setProgress] = useState<any[]>([]);                                              // 进度条列表\n    const [flag, setFlag] = useState<boolean>(false);                                                 // 进度模态框\n\n\n    let uploadQueue;\n\n    const onCancel = () => {\n        setFlag(false);\n        setUploadingQueue([]);\n        setProgress([]);\n        queueFileList = [];\n        uploadDone = false;\n        props.callbackCancel();\n    };\n\n    const callback = (file) => {\n        let arr = [];\n        if (uploadingQueue.length > 0) {\n            uploadingQueue.map((item) => {\n                if (item.uid !== file.uid) {\n                    arr.push(item);\n                }\n            });\n        }\n\n        // return arr;\n        uploadQueue(queueFileList, arr);\n    };\n\n    uploadQueue = (queue, uploadingQueue) => {\n\n        console.log(queue, 'queue------------------');\n        console.log(uploadingQueue, 'uploadingQueue------------------');\n        let processing = [];\n        if (uploadingQueue.length < maxNum && queue.length > 0) {\n            if (queue.length > maxNum) {\n                processing = queue.splice(0, maxNum - uploadingQueue.length);\n            } else {\n                processing = [...queue];\n                queue = [];\n            }\n        }\n\n        processing.map((item) => {\n            progress.push(<Progress key={item.uid} file={item} params={params} callback={callback} url={url} />);\n        });\n\n        queueFileList = queue;\n        setUploadingQueue(processing);\n        setProgress(progress);\n        if (processing.length === 0 && !uploadDone) {\n            props.callback({ status: 'success', message: '上传成功' });\n            uploadDone = true;\n            setProgress([]);\n            setFlag(false)\n        }\n    };\n\n\n    /**\n     * @description 文件上传\n     */\n    const doUpload = (file, fileList) => {\n        let moreMaxSize = false;\n        if (Array.isArray(fileList)) {\n            fileList.map((item) => {\n                console.log(item, 'item');\n                if (item.size > MAX_FILE_SIZE) {\n                    moreMaxSize = true;\n                }\n            });\n            console.log(moreMaxSize, 'moreMaxSize');\n            if (moreMaxSize) {\n                onCancel();\n                props.callback({ status: 'error', message: '请上传小于100M的文件' });\n            } else {\n                uploadQueue(fileList, uploadingQueue);\n                setFlag(true);\n            }\n        }\n    };\n\n\n    const upload_debounce = lodash.debounce(doUpload, 100);\n\n    return (\n        <>\n            <Upload\n                customRequest={true}\n                fileList={[]}\n                beforeUpload={upload_debounce}\n                {...uploadProps}\n            >\n                {children}\n            </Upload>\n            <Modal\n                title=\"文件上传\"\n                visible={flag}\n                footer={null}\n                width={600}\n                onCancel={onCancel}\n            // style={{ height: 350 }}\n            >\n                <div style={{ minHeight: 300, maxHeight: 400, overflow: 'auto' }}>\n                    {progress.map((item) => item)}\n                </div>\n            </Modal>\n        </>\n\n    );\n};\n\n\nexport default UploadProgress;\n","import React, { useEffect, useState } from 'react';\r\nimport { Table } from 'antd';\r\nimport MXTable from '@/pages/components/MXTable';\r\n\r\nimport styles from './index.less'\r\ninterface propsTs {\r\n  dataSource:array,\r\n  total:number,\r\n  loading:boolean,\r\n  onChange?:any,\r\n  onSelectChange?:any\r\n}\r\n\r\nconst statusObj = {\r\n  0:'未解析',\r\n  1:'已解析',\r\n  2:'解析失败'\r\n}\r\n\r\nconst AuthAdd: React.FC<propsTs> = props => {\r\n  const [visible, setvisible] = useState<boolean>(false);\r\n  const [selectedRowKeys, setselectedRowKeys] = useState<array>([]);\r\n  const [pageData,setpageData] = useState<object>({\r\n    // 当前的分页数据\r\n    pageNum: 1,\r\n    pageSize: 10\r\n  })\r\n\r\n  const {dataSource,total,loading,onChange,onSelectChange} = props\r\n\r\n  const onTableSelectChange=(values)=>{\r\n    setselectedRowKeys([...values])\r\n    onSelectChange(values)\r\n  }\r\n\r\n  const rowSelection = {\r\n    selectedRowKeys,\r\n    