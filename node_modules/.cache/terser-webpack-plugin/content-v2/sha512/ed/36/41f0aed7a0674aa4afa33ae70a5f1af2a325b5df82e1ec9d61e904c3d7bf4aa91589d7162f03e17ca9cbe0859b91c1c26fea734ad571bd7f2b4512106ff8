  <>\n                                                {fields.map((field, i) => (\n                                                    <Row gutter={8} key={i}>\n\n                                                        <Col span={22}>\n                                                            <Form.Item\n                                                                {...formItemLayout1}\n                                                                label={`问题${i + 1}`}\n                                                                extra={'要求回答:是'}\n                                                                {...field}\n                                                                name={[field.name, 'questionContent']}\n                                                                rules={[\n                                                                    {\n                                                                        required: true,\n                                                                        whitespace: true,\n                                                                        message: '请输入问题'\n                                                                    }\n                                                                ]}\n                                                            >\n                                                                <Input disabled={isEdit} />\n                                                            </Form.Item>\n                                                        </Col>\n\n                                                        {!isEdit &&\n                                                            <Col span={2}>\n                                                                <span\n                                                                    className={_styles.addIcon}\n                                                                    title=\"在此问题后面添加一个问题\"\n   