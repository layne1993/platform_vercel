{
  "name": "sass",
  "version": "4.1.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "build:vipsit": "cross-env REACT_APP_ENV=vipsit umi build",
    "build:shjiu": "cross-env REACT_APP_ENV=shjiu umi build",
    "build:prod": "cross-env REACT_APP_ENV=prod umi build",
    "build:mh": "cross-env REACT_APP_ENV=mh umi build",
    "build:mshi": "cross-env REACT_APP_ENV=mshi umi build",
    "build:hd": "cross-env REACT_APP_ENV=hd umi build",
    "build:jhl": "cross-env REACT_APP_ENV=jhl umi build",
    "build:ly": "cross-env REACT_APP_ENV=ly umi build",
    "build:zho": "cross-env REACT_APP_ENV=zho umi build",
    "build:vip": "cross-env REACT_APP_ENV=vip umi build",
    "build:xw": "cross-env REACT_APP_ENV=xw umi build",
    "build:jk": "cross-env REACT_APP_ENV=jk umi build",
    "build:qingli": "cross-env REACT_APP_ENV=qingli umi build",
    "build:hwen": "cross-env REACT_APP_ENV=hwen umi build",
    "build:ningq": "cross-env REACT_APP_ENV=ningq umi build",
    "build:ws": "cross-env REACT_APP_ENV=ws umi build",
    "build:fe": "cross-env REACT_APP_ENV=fe umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "prod": "npm run start:prod",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start": "cross-env UMI_UI=none umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_UI=none umi dev",
    "start:hd": "cross-env REACT_APP_ENV=hd MOCK=none UMI_UI=none umi dev",
    "start:shjiu": "cross-env REACT_APP_ENV=shjiu MOCK=none UMI_UI=none umi dev",
    "start:mshi": "cross-env REACT_APP_ENV=mshi MOCK=none UMI_UI=none umi dev",
    "start:vip": "cross-env REACT_APP_ENV=vip MOCK=none UMI_UI=none umi dev",
    "start:zho": "cross-env REACT_APP_ENV=zho MOCK=none UMI_UI=none umi dev",
    "start:jk": "cross-env REACT_APP_ENV=jk MOCK=none UMI_UI=none umi dev",
    "start:jhl": "cross-env REACT_APP_ENV=jhl MOCK=none UMI_UI=none umi dev",
    "start:prod": "cross-env REACT_APP_ENV=prod MOCK=none UMI_UI=none umi dev",
    "start:ningq": "cross-env REACT_APP_ENV=ningq MOCK=none umi dev",
    "start:hwen": "cross-env REACT_APP_ENV=hwen MOCK=none umi dev",
    "start:ws": "cross-env REACT_APP_ENV=ws MOCK=none umi dev",
    "start:fe": "cross-env REACT_APP_ENV=fe MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_UI=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
